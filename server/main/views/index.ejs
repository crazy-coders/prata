  <script src="/jquery/jquery.min.js"></script>
  <script src="/handlebars/handlebars.js"></script>
  
  <body>
    <script type="text/x-handlebars" data-template-name="messages">
      <section id="prataapp">
        <header id="header">
          <h1>prata</h1>
          {{textarea id="new-message" placeholder="What needs to be told?" value=newMessage action="createMessage"}}
        </header>
        
        <section id="main">
          <div id="message-list">
            {{#each}}
              <div>
                <p><strong>{{user.name}}</strong></p>
                <p>{{message}}</p>
              </div>
            {{/each}}
          </div>
        </section>
        
      </section>
    </script>
    

    <!-- ... Ember.js and other javascript dependencies ... -->
